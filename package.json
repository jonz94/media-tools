{
  "name": "media-tools",
  "version": "0.3.1",
  "private": true,
  "description": "Media tools build on top of open source giants",
  "homepage": "https://github.com/jonz94/media-tools#readme",
  "bugs": {
    "url": "https://github.com/jonz94/media-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonz94/media-tools.git"
  },
  "license": "Unlicense",
  "author": {
    "name": "jonz94",
    "email": "jody16888@gmail.com"
  },
  "main": "main.js",
  "scripts": {
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "cross-env NODE_ENV=production npm run build -- -c production",
    "electron:build": "npm run build:prod && cross-env DEBUG=electron-builder electron-builder build",
    "electron:local": "npm run build:prod && electron .",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "postinstall": "electron-builder install-app-deps",
    "ng:serve": "cross-env WEBPACK_TARGET=web ng serve -c web",
    "start": "npm-run-all -p electron:serve ng:serve",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "web:serve": "npm run ng:serve -- -o"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "youtube-dl": "^3.0.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1100.2",
    "@angular/cli": "^11.0.2",
    "@angular/common": "^11.0.2",
    "@angular/compiler": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/core": "^11.0.2",
    "@angular/forms": "^11.0.2",
    "@angular/platform-browser": "^11.0.2",
    "@angular/platform-browser-dynamic": "^11.0.2",
    "@angular/router": "^11.0.2",
    "@ionic/angular": "^5.5.0",
    "@ionic/angular-toolkit": "^3.0.0",
    "@jonz94/prettier-config": "^1.0.1",
    "@ngneat/transloco": "^2.19.1",
    "@types/jasmine": "^3.6.2",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.19.6",
    "@types/youtube-dl": "^1.12.2",
    "autoprefixer": "^10.0.2",
    "codelyzer": "^6.0.1",
    "conventional-changelog-cli": "^2.1.1",
    "core-js": "^3.7.0",
    "cross-env": "^7.0.2",
    "electron": "^10.1.6",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0",
    "ionicons": "^5.2.3",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.2.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-electron": "^6.3.1",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.3",
    "postcss-scss": "^3.0.2",
    "prettier": "^2.2.0",
    "prettier-plugin-organize-imports": "^1.1.1",
    "prettier-plugin-packagejson": "^2.2.7",
    "rxjs": "^6.6.3",
    "tailwindcss": "^1.9.6",
    "ts-node": "^9.0.0",
    "tslib": "^2.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "wait-on": "^5.2.0",
    "zone.js": "~0.10.3"
  }
}
